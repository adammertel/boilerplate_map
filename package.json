{
  "name": "boilerplate_map",
  "version": "0.1.0",
  "description": "boilerplate for react-leaflet map applications",
  "author": "Adam Mertel <mertel.adam@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adammertel/boilerplate_map/issues"
  },
  "homepage": "https://github.com/adammertel/boilerplate_map#readme",
  "main": "index.js",
  "scripts": {
    "clean-dist": "rm -f -r -d dist",
    "start": "webpack-dev-server --open --config webpack.development.js",
    "build": "webpack --config webpack.production.js",
    "deploy": "rsync -avz build/ <user@url:/path> --delete",
    "build-deploy": "npm run build && npm run deploy",
    "build-tailwind": "./node_modules/.bin/tailwind build ./app/styles.css -c ./tailwind.js -o ./build/styles.css",
    "test": "jest --detectOpenHandles",
    "test-unit": "jest --detectOpenHandles --testPathPattern=\".*/tests/unit/.*.spec.ts\" ",
    "test-e2e": "jest --detectOpenHandles --testPathPattern=\".*/tests/e2e/.*.spec.ts\" ",
    "test-component": "jest --detectOpenHandles --updateSnapshot --testPathPattern=\".*/tests/component/.*.spec.tsx\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adammertel/boilerplate_map.git"
  },
  "keywords": [
    "boilerplate",
    "leaflet",
    "react-leaflet",
    "mobx"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "favicons-webpack-plugin": "^4.2.0",
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-leaflet": "^2.7.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.13",
    "@types/jest-environment-puppeteer": "^4.3.2",
    "@types/node": "^14.6.3",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "clean-webpack-plugin": "^3.0.0",
    "components": "^0.1.0",
    "css-loader": "^4.2.2",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "file-loader": "^6.1.0",
    "gts": "^2.0.2",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "jest-environment-puppeteer": "^4.4.0",
    "leaflet": "^1.7.0",
    "mini-css-extract-plugin": "^0.11.0",
    "postcss-cli": "^7.1.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "puppeteer": "^5.2.1",
    "react-hot-loader": "^4.12.21",
    "style-loader": "^1.2.1",
    "tailwindcss": "^1.7.6",
    "tailwindcss-classnames": "^1.6.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  }
}
