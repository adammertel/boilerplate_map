{
  "name": "boilerplate_map",
  "version": "0.0.1",
  "description": "boilerplate for react-leaflet map applications",
  "author": "Adam Mertel <mertel.adam@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adammertel/boilerplate_map/issues"
  },
  "homepage": "https://github.com/adammertel/boilerplate_map#readme",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "clean-dist": "rm -f -r -d dist",
    "start": "parcel app/index.html --open --no-cache",
    "build": "npm run clean-dist && parcel build app/index.html --no-cache --public-url ./",
    "deploy": "rsync -avz build/ <user@url:/path> --delete",
    "build-deploy": "npm run build && npm run deploy",
    "build-tailwind": "./node_modules/.bin/tailwind build ./app/styles.css -c ./tailwind.js -o ./build/styles.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adammertel/boilerplate_map.git"
  },
  "keywords": [
    "boilerplate",
    "leaflet",
    "react-leaflet",
    "mobx"
  ],
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "components": "^0.1.0",
    "leaflet": "^1.5.1",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "postcss-custom-properties": "^8.0.10",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-leaflet": "^2.2.1",
    "sass": "^1.19.0",
    "tailwindcss": "^0.7.4",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "cssnano": "^4.1.10"
  }
}
