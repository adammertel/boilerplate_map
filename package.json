{
  "name": "boilerplate_map",
  "version": "0.0.1",
  "description": "boilerplate for react-leaflet map applications",
  "author": "Adam Mertel <mertel.adam@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adammertel/boilerplate_map/issues"
  },
  "homepage": "https://github.com/adammertel/boilerplate_map#readme",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "clean-build": "rm -f -r -d build",
    "start": "parcel app/index.html --open --out-dir build",
    "build": "yarn run clean-build && parcel build app/index.html --out-dir build --no-cache --public-url ./",
    "deploy": "rsync -avz build/ hde@hde.geogr.muni.cz:/var/www/virthtml/hde/cisterciansfrance --delete",
    "build-deploy": "npm run build && npm run deploy"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adammertel/boilerplate_map.git"
  },
  "keywords": [
    "boilerplate",
    "leaflet",
    "react-leaflet",
    "mobx"
  ],
}
