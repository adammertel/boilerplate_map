{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",

    "baseUrl": "./app",
    "paths": {
      "*": ["./app/*"]
    }
  },
  "include": ["app/*.*", "app/**/*.*", "test/*.ts", "test/**/*.ts"],
  "exclude": [".git", "node_modules"]
}
